extends layout

block content
  h1= title
  
  p Enter #{title} details

  form(action='saveData' method='get')
    .row
      .input-field.col.s6
        label.active(for='name') Name
        input#name.validate( type='text', name='name')
    .row
      .input-field.col.s6
        label.active(for='phoneNo') Phone Number
        input#phoneNo.validate( type='text', name='phoneNo')
        
    .row
      button.btn(type='submit') Save